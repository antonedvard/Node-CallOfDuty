{
  "name": "call-of-duty-api",
  "version": "1.3.0",
  "description": "Wrapper for the Call Of Duty API.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --trace-warnings test/run.js",
    "docs:build": "typedoc --options ./typedoc.js && vuepress build ./docs --no-cache",
    "dev": "NODE_ENV=development ",
    "test": "npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonedvard/act-cod-api.git"
  },
  "keywords": [
    "br",
    "warzone",
    "call-of-duty",
    "call of duty",
    "modern warfare",
    "black ops",
    "iw",
    "battle royale"
  ],
  "author": "Liam Gaskell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antonedvard/act-cod-api/issues"
  },
  "homepage": "https://github.com/antonedvard/act-cod-api#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "axios-rate-limit": "^1.2.1",
    "uniqid": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/uniqid": "^5.2.0",
    "dotenv": "^8.2.0",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.4.0",
    "typescript": "^3.9.7",
    "vuepress": "^1.5.3"
  }
}
